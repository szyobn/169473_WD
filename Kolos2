zad1
import math
import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

x = np.arange(2, 9.5, 0.5)
y = np.sqrt(x + 3*x) / (4*x + 5)
plt.plot(x, y,'r',label='f(x) = (sqrt(x)+3*x) / (4x + 5)')
plt.xlabel('x')
plt.ylabel('f(x)')
plt.title('Wykres funkcji f(x)')
plt.legend()
plt.xlim(2, 9)
plt.show()
